  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Convert example .gltf file",
      "runtimeVersion": "16",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bin/omi-gltf-transform.js",
      "args": ["./assets/OutdoorFestival/gltf/OutdoorFestival.gltf", "./out/OutdoorFestival_out.gltf"],
    }
  ]
}